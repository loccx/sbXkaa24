{
  "manifest_version": 3,
  "name": "Background Script Test",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "hello_extensions.png"
  },
  "permissions": [
    "activeTab"
  ]
}

